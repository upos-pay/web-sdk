{
  "name": "@upos-pay/web-sdk",
  "version": "1.0.3",
  "description": "Official Web SDK for UPOS payments",
  "author": "UPOS",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@4.12.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upos-pay/web-sdk.git"
  },
  "homepage": "https://upos.fi",
  "bugs": {
    "url": "https://github.com/upos-pay/web-sdk/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "default": "./dist/react.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "./dist/index.js"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "release": "npm publish --access public"
  },
  "dependencies": {
    "@lit/context": "^1.1.6",
    "@lit/react": "^1.0.8",
    "js-base64": "^3.7.8",
    "lit": "^3.3.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/node": "^22.19.3",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-perfectionist": "^5.0.0",
    "sass": "^1.97.1",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "payment",
    "sdk",
    "upos",
    "crypto",
    "tron"
  ]
}
